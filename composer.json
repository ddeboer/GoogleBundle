{
    "name": "znanylekarz/google-bundle",
    "description": "The GoogleBundle adds the ability to add various google-related services to your application. These include Google Analytics, Adwords and Static Maps.",
    "keywords": ["google", "analytics", "adwords", "maps"],
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name":     "antimattr",
            "homepage": "https://github.com/antimattr"
        }
    ],
    "require": {
        "php": "^7.4 || ^8.0",
        "symfony/config": "^3.0 || ^4.0",
        "symfony/dependency-injection": "^3.0 || ^4.0",
        "symfony/http-kernel": "^3.0 || ^4.0",
        "symfony/templating": "^3.0 || ^4.0",
        "symfony/http-foundation": "^3.0 || ^4.0",
        "doctrine/collections": "^1.0|^2.0",
        "twig/twig": "^1.44.7 || ^2 || ^3"
    },
    "require-dev": {
        "phpunit/phpunit": "^9",
        "symfony/framework-bundle": "^3.0 || ^4.0",
        "symfony/yaml": "^3.0 || ^4.0",
        "symfony/twig-bundle": "^3.0 || ^4.0",
        "symfony/twig-bridge": "^3.0 || ^4.0",
        "symfony/browser-kit": "^3.0 || ^4.0"
    },
    "autoload": {
        "psr-0": {
            "AntiMattr\\GoogleBundle": ""
        }
    },
    "target-dir" : "AntiMattr/GoogleBundle"
}
